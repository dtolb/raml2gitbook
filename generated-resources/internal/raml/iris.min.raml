                            201:
                                description: Successful creation of report instance.
                                headers:
                                    Location:
                                        description: Contains URL of newly-created report.
                                        example: https://dashboard.bandwidth.com/api/accounts/123/reports/456/instances/789
                                body:
                                    application/xml:
                                        example: None
                            400:
                                body:
                                    application/xml:
