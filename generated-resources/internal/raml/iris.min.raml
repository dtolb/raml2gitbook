                    body:
                        application/xml:
                            example: |
                                <Product>
                                    <Name>UcTrunking</Name>
                                    <Features>
                                        <Feature>CallForwarding</Feature>
                                        <Feature>CNAM</Feature>
                                        <Feature>LIDB</Feature>
                                        <Feature>DlDa</Feature>
                                    </Features>
                                </Product>
